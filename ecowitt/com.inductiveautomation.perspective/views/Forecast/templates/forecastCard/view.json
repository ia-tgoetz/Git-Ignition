                "struct": {
                  "day": "{view.params.isDaytime}",
                  "desc": "{view.params.shortForecast}",
                  "list": "{view.custom.level}"
                },
                "waitOnAll": true
              },
              "transforms": [
                {
                  "code": "\tshortForecast\u003dvalue[\u0027desc\u0027].replace(\" \",\"\")\n\tremoveList\u003d[\u0027Slight\u0027,\u0027Chance\u0027,\u0027Likely\u0027]\n\tif value[\u0027day\u0027]:\n\t\tIconlist\u003dvalue[\u0027list\u0027].Day\n\telse:\n\t\tIconlist\u003dvalue[\u0027list\u0027].Night\n\tfor x in removeList:\n\t\tshortForecast\u003dshortForecast.replace(x,\"\")\n\tif shortForecast in Iconlist.keys():\n\t\treturn Iconlist[str(shortForecast)]\n\treturn Iconlist[\"NotAvailable\"]",
                  "type": "script"
                }
              ],
              "type": "expr-struct"
            }
          }
