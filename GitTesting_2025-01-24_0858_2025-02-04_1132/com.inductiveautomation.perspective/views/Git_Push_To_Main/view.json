{
  "custom": {},
  "params": {
    "directory": "/usr/local/bin/ignition/data/projects"
  },
  "propConfig": {
    "params.directory": {
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 115
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "200px",
              "shrink": 0
            },
            "props": {
              "style": {
                "marginRight": "4px"
              },
              "text": "Commit Message",
              "textStyle": {
                "textAlign": "right"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "TextField"
            },
            "position": {
              "basis": "150px",
              "grow": 1
            },
            "props": {
              "deferUpdates": false,
              "text": "myCommit"
            },
            "type": "ia.input.text-field"
          }
        ],
        "meta": {
          "name": "Commit_Message"
        },
        "position": {
          "basis": "36px",
          "shrink": 0
        },
        "props": {
          "style": {
            "margin": "4px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\tlogger\u003dsystem.util.getLogger(\u0027GitHub\u0027)\n\tcommitMessage\u003dself.parent.getChild(\"Commit_Message\").getChild(\"TextField\").props.text\n\tdirectory\u003dself.view.params.directory\n\tgitScripts.push_to_main(directory, commitMessage)\n\tself.props.value\u003d0"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "OneShotButton_0"
        },
        "position": {
          "basis": "45px",
          "shrink": 0
        },
        "props": {
          "readyState": {
            "icon": {
              "style": {
                "height": "24px",
                "width": "2rem"
              }
            },
            "text": "Set Remote Repo"
          },
          "style": {
            "margin": "4px",
            "marginLeft": "208px"
          },
          "writingState": {
            "icon": {
              "path": "material/hourglass_empty",
              "style": {
                "height": "24px",
                "width": "2rem"
              }
            },
            "text": "Creating Remote Repo..."
          }
        },
        "type": "ia.input.oneshotbutton"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}