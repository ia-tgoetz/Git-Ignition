        "meta": {
          "name": "SetRemote_1"
        },
        "position": {
          "basis": "185px"
        },
        "propConfig": {
          "props.enabled": {
            "binding": {
              "config": {
                "path": "view.custom.PATSaved"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "image": {
            "icon": {
              "path": "material/cloud_download"
            }
          },
          "primary": false,
          "style": {
            "margin": "2px"
          },
          "text": "Pull From Repo"
        },
        "type": "ia.input.button"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "justify": "center"
    },
    "scripts": {
      "customMethods": [],
      "extensionFunctions": null,
      "messageHandlers": [
        {
          "messageType": "token",
          "pageScope": true,
          "script": "\timport os\n\tself.view.custom.PATSaved\u003dos.path.exists(\"/usr/local/bin/ignition/data/tmp/gitPAT.json\")",
          "sessionScope": false,
          "viewScope": false
        }
      ]
    },
    "type": "ia.container.flex"
  }
