              "scope": "C",
              "type": "popup"
            }
          }
        },
        "meta": {
          "name": "SetRemote"
        },
        "position": {
          "basis": "185px"
        },
        "propConfig": {
          "props.enabled": {
            "binding": {
              "config": {
                "path": "view.custom.PATSaved"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "image": {
            "icon": {
              "path": "material/file_copy"
            }
          },
          "primary": false,
          "style": {
            "margin": "2px"
          },
          "text": "Setup Repo"
        },
        "type": "ia.input.button"
      },
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "draggable": true,
                "id": "Push",
                "modal": false,
                "overlayDismiss": false,
                "resizable": true,
                "showCloseIcon": true,
                "title": "Push to Remote",
                "type": "open",
                "viewPath": "Exchange/gitIgnition/EmbeddedViews/GitPushToBranch",
                "viewportBound": false
              },
              "scope": "C",
              "type": "popup"
            }
          }
        },
        "meta": {
          "name": "SetRemote_0"
        },
        "position": {
          "basis": "185px"
        },
        "propConfig": {
          "props.enabled": {
            "binding": {
              "config": {
                "path": "view.custom.PATSaved"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "image": {
            "icon": {
              "path": "material/double_arrow"
            }
          },
          "primary": false,
          "style": {
            "margin": "2px"
          },
          "text": "Push to Repo"
