{
  "custom": {
    "PATSaved": false
  },
  "events": {
    "system": {
      "onStartup": {
        "config": {
          "script": "\timport os\n\tself.custom.PATSaved\u003dos.path.exists(\"/usr/local/bin/ignition/data/tmp/gitPAT.json\")"
        },
        "scope": "G",
        "type": "script"
      }
    }
  },
  "params": {},
  "props": {
    "defaultSize": {
      "height": 45
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "draggable": true,
                "id": "PAT",
                "modal": false,
                "overlayDismiss": false,
                "resizable": true,
                "showCloseIcon": true,
                "title": "Set Personal Access Token",
                "type": "open",
                "viewPath": "Exchange/gitIgnition/EmbeddedViews/storeGitToken",
                "viewportBound": false
              },
              "scope": "C",
              "type": "popup"
            }
          }
        },
        "meta": {
          "name": "SetToken"
        },
        "position": {
          "basis": "185px"
        },
        "propConfig": {
          "props.image.icon.color": {
            "binding": {
              "config": {
                "path": "view.custom.PATSaved"
              },
              "transforms": [
                {
                  "fallback": "red",
                  "inputType": "scalar",
                  "mappings": [
                    {
                      "input": true,
                      "output": "green"
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          },
          "props.image.icon.path": {
            "binding": {
              "config": {
                "path": "view.custom.PATSaved"
              },
              "transforms": [
                {
                  "fallback": "material/lock",
                  "inputType": "scalar",
                  "mappings": [
                    {
                      "input": true,
                      "output": "material/check"
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "image": {
            "icon": {}
          },
          "primary": false,
          "style": {
            "margin": "2px"
          },
          "text": "Set Personal Token"
        },
        "type": "ia.input.button"
      },
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "draggable": true,
                "id": "Remote",
                "modal": false,
                "overlayDismiss": false,
                "resizable": true,
                "showCloseIcon": true,
